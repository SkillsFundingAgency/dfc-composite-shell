<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
	  <security>
      <requestFiltering removeServerHeader="true" />
    </security>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
		<add name="X-XSS-Protection" value="0" />
	  </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>