@{
    Layout = Model.LayoutName;
    ViewBag.BrandingAssetsCdn = Model.BrandingAssetsCdn;
    ViewBag.Title = Model.PageTitle;
    ViewBag.PhaseBannerHtml = Model.PhaseBannerHtml;

    ViewBag.VersionedPathForMainMinCss = Model.VersionedPathForMainMinCss;
    ViewBag.VersionedPathForGovukMinCss = Model.VersionedPathForGovukMinCss;
    ViewBag.VersionedPathForAllIe8Css = Model.VersionedPathForAllIe8Css;
    ViewBag.VersionedPathForJQueryBundleMinJs = Model.VersionedPathForJQueryBundleMinJs;
    ViewBag.VersionedPathForAllMinJs = Model.VersionedPathForAllMinJs;
    ViewBag.VersionedPathForDfcDigitalMinJs = Model.VersionedPathForDfcDigitalMinJs;
}

@{
    DefineSection("Head", async (a) =>
    {
        if (Model.ContentHead != null)
        {
            this.WriteLiteral(Model.ContentHead.Value);
        }
    });
}

