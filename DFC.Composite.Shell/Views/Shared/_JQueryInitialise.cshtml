@{
    var versionedPathForJavaScripts = ViewData["VersionedPathForJavaScripts"] as List<string>;

    foreach (var script in versionedPathForJavaScripts)
    {
        <script type="module" src="@script" type="text/javascript" nws-csp-add-nonce="true"></script>
        <script type="module">
            import { initAll } from '@script'
            initAll()
        </script>
    }
}